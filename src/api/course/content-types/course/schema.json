{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Description": {
      "type": "richtext"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "Published": {
      "type": "boolean",
      "default": false
    },
    "Duration": {
      "type": "integer"
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "category"
    },
    "URL": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}
